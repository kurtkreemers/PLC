<Window x:Class="HmiExample.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="100" Width="200" ResizeMode="NoResize">
    <StackPanel>
    <Label>Administrator password!!!</Label>
    <PasswordBox Name="passwordBox" Height="20" Width="100" VerticalAlignment="Center" Password="pass" KeyDown="passwordBox_KeyDown"></PasswordBox>
    </StackPanel>
</Window>
