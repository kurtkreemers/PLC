<Window x:Class="HmiExample.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="125" Width="200" ResizeMode="NoResize">
    <StackPanel>
    <Label>Login :</Label>
        <ComboBox Name="comboboxUserLogin" Width="100" Height="20" ></ComboBox> 
    <PasswordBox Name="passwordBox" Height="20" Width="100" VerticalAlignment="Center" Password="pass" KeyDown="passwordBox_KeyDown" Margin="15"></PasswordBox>
    </StackPanel>
</Window>
